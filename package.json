    "babel-register": "^6.26.0",
    "chokidar": "^1.7.0",
    "got": "^8.0.1",
    "meow": "^4.0.0",
    "opn": "^5.1.0",
    "ora": "^1.3.0",
    "portfinder": "^1.0.13",
